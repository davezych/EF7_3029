{
  "webroot": "wwwroot",
  "version": "1.0.0-*",

  "dependencies": {
    "EntityFramework.SqlServer": "7.0.0-rc1-15886",
    "EntityFramework.Core": "7.0.0-rc1-15886",
    "EntityFramework.Commands": "7.0.0-rc1-15886",
    "Microsoft.AspNet.Mvc.Core": "6.0.0-rc1-15954",
    "Microsoft.AspNet.Mvc": "6.0.0-rc1-15954"
  },

  "commands": {
    "web": "Microsoft.AspNet.Hosting server=Microsoft.AspNet.Server.WebListener server.urls=http://localhost:5002",
    "ef": "EntityFramework.Commands"
